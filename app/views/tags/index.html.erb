<h1>Tags#index</h1>

<br>

<% extend Pagy::Frontend %>
<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead class="table-light">
    <tr>
      <th>Title</th>
      <th>Created at</th>
    </tr>
    </thead>
    <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.title %></td>
        <td><%= tag.created_at %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= render "shared/pagination" %>